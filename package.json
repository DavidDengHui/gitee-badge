{
  "name": "badge-next",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "prepare": "husky install && husky set .husky/pre-commit \"npx lint-staged\"",
    "format": "prettier --write .",
    "lint": "next lint"
  },
  "dependencies": {
    "@wtto00/spider-crawler": "0.0.1-beta.5",
    "cheerio": "1.0.0-rc.12",
    "marked": "^4.0.19",
    "next": "12.2.5",
    "raw-loader": "^4.0.2",
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "devDependencies": {
    "@types/marked": "^4.0.6",
    "@types/node": "18.7.13",
    "@types/qs": "^6.9.7",
    "@types/react": "18.0.17",
    "@typescript-eslint/eslint-plugin": "^5.34.0",
    "@typescript-eslint/parser": "^5.34.0",
    "eslint": "8.22.0",
    "eslint-config-next": "12.2.5",
    "eslint-plugin-react-hooks": "^4.6.0",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.3",
    "prettier": "^2.7.1",
    "sass": "^1.54.5",
    "typescript": "4.7.4"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "prettier --write",
      "eslint --color --fix",
      "git add"
    ],
    "*.{css,scss}": [
      "prettier --write",
      "git add"
    ]
  }
}
