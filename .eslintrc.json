{
  "root": true,
  "env": {
    "browser": true,
    "es2021": true,
    "node": true
  },
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    },
    "ecmaVersion": 12,
    "sourceType": "module",
    "project": ["./tsconfig.json"]
  },
  "extends": ["airbnb", "airbnb-typescript", "next/core-web-vitals"],
  "rules": {
    "no-param-reassign": ["error", { "props": false }]
  }
}
