{
  "name": "badg",
  "version": 2,
  "alias": ["badg.now.sh"],
  "functions": {
    "api/gitee/[subject]/[owner]/[repo]/index.js": {
      "memory": 3008,
      // Maximum Execution Duration: 10s (Hobby), 60s (Pro), or 900s (Enterprise)
      "maxDuration": 60
    }
  },
  "cleanUrls": true,
  "rewrites": [
    { "source": "/gitee/*", "destination": "/api/gitee" },
    { "source": "/badge/", "destination": "/api/badge/" }
  ]
}
