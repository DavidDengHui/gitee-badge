{
  "name": "badg",
  "version": 2,
  "alias": ["badg.now.sh"],
  "functions": {
    "api/gitee/[subject]/[owner]/[repo]/index.js": {
      "memory": 3008,
      // Maximum Execution Duration: 10s (Hobby), 60s (Pro), or 900s (Enterprise)
      "maxDuration": 60
    }
  },
  "regions": ["hkg1"],
  "routes": [{ "src": "/gitee/.*", "dest": "/api/gitee/$1" }]
}
